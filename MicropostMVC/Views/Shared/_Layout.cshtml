@{
    ViewBag.UrlProject = "https://github.com/marcelopuppin/micropostsmvc";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title - Microposts .NET MVC</title>
    <link href="@System.Web.Optimization.BundleTable.Bundles.ResolveBundleUrl("~/Content/css")" rel="stylesheet" type="text/css" />
    <link href="@System.Web.Optimization.BundleTable.Bundles.ResolveBundleUrl("~/Content/themes/base/css")" rel="stylesheet" type="text/css" />
    <script src="@System.Web.Optimization.BundleTable.Bundles.ResolveBundleUrl("~/Scripts/js")" type="text/javascript"></script>
</head>
<body>
    <div class="container">
      <header>
        @RenderPage("_Header.cshtml")
      </header>
      <section class="round">
        @RenderBody()
      </section>
      <footer>
        @RenderPage("_Footer.cshtml")
      </footer>
    </div>
</body>
</html>
